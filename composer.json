{
    "name": "malik12tree/zatca",
    "description": "PHP implementation of ZATCA E-Invoicing standards",
    "version": "0.1.0",
    "type": "library",
    "keywords": [
        "zatca",
        "invoicing",
        "egs",
        "qrcode"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Malik12tree"
        }
    ],
    "autoload": {
        "psr-4": {
            "Malik12tree\\ZATCA\\": "src/"
        },
        "files": [
            "src/Utils/functions.php"
        ]
    },
    "require": {
        "mpdf/mpdf": "8.2.4",
        "mpdf/qrcode": "1.2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix"
    }
}
